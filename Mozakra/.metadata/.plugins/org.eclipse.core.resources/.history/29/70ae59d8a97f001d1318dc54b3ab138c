#include <stdio.h>
#include <stdlib.h>
void string(char *str)
{
	int size=0,i=0;
	size=strlen(str);
	for(i=size;i>0;i++)
	{
		if((i==(size-1))||(i==(size-2))
		{
			printf("%c",str[i]);
		}
	}
}
int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	char *str="hussin";
	string(str);
	return 0;
}

