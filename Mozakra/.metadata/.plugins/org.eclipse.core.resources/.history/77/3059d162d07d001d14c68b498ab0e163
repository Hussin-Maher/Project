#include <stdio.h>
#include <stdlib.h>
int convert(char *arr)
{
	int i=0;
	while(arr[i]!='\0')
	{
		i++;
	}
	return (i-1);
}
int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	int i=0;
	char str[]="HUSSIN";
	i=convert(str);
	printf("%d",i);
	return 0;
}

