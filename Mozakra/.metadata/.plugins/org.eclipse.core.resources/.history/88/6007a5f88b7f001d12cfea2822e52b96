#include <stdio.h>
#include <stdlib.h>
void reversing(int *arr,int size)
{
	int i=0,min=arr[0];
	for(i=0;i<size;i++)
	{
		if (arr[i] < min) {
			min=arr[i];
		}
	}
	printf("%d",min);
}
int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	int arr[3]={9,2,0};
	reversing(arr, 3);
	return 0;
}

