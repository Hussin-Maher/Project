#include <stdio.h>
#include <stdlib.h>
int factorial(int x)
{
	if(x==1)
		return x;
	else
	return x*factorial(x-1);
}
int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	int z=0;
	z=factorial(5);
	printf("%d",z);
	return 0;
}

