#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	int x=0,r=0;
	scanf("%d",&x);
	while(x!=0)
	{
		r=x%10;
		printf("%d",r);
		x=x/10;
	}

	return EXIT_SUCCESS;
}
