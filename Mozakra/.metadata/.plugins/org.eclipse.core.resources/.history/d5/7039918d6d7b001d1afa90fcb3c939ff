#include <stdio.h>
#include <stdlib.h>
int cube(int);
int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	int x=0,r;
	scanf("%d",&x);
	r=cube(x);
	printf("%d",r);
	return 0;
}
int cube(int x)
{
	int z=0;
	z=x*x;
	return z;
}
