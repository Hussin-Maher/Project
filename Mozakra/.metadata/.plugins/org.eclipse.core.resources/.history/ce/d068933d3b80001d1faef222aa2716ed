int size=9;
#include <stdio.h>
#include <stdlib.h>


int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	int x=0xFFFFAAAA;
	int y=0;
	char *ptr1=&x;
	ptr1++;
	y=*ptr1;

	printf("%x",y);
	return 0;
}

