#include <stdio.h>
#include <stdlib.h>
void convert(char *arr)
{
	int i=0;
	while(arr[i]!='\0')
	{
		arr[i]=arr[i]+32;
		i++;
	}
}
int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	int i=0;
	char str[]="HUSSIN";
	convert(str);
	while(str[i]!='\0')
	{
		printf("%c",str[i]);
		i++;
	}
	return 0;
}

