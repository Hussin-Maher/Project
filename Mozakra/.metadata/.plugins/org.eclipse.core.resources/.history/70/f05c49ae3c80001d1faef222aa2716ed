int size=9;
#include <stdio.h>
#include <stdlib.h>


int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	int x=0xFFFFAAAA;
	int y=0;
	short *ptr1=&x;
	y=ptr1[1];
	y=y&ptr1[1];
	printf("%x",y);
	return 0;
}

