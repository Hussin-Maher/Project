#include <stdio.h>
#include <stdlib.h>
void floor(float,float);
int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	float x,y;
	scanf("%f%f",&x,&y);
	floor(x,y);
	return 0;
}
void floor(float x,float y)
{
	int z;
	z=(int)x+y;
	printf("%d",z);
}
